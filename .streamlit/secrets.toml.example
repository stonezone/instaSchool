# Streamlit Cloud Secrets Configuration
# Copy these to your Streamlit Cloud app settings -> Secrets

# OpenAI (for images + cheap text models)
OPENAI_API_KEY = "sk-your-openai-api-key-here"
OPENAI_ORG_ID = "org-your-org-id-here"

# Kimi K2 (Moonshot) (recommended for text generation)
# The app supports either env var name; MOONSHOT_API_KEY is the official one.
MOONSHOT_API_KEY = "sk-your-moonshot-api-key-here"
KIMI_API_KEY = "sk-your-moonshot-api-key-here"  # optional alias

# DeepSeek (cheap reasoning models)
DEEPSEEK_API_KEY = "sk-your-deepseek-api-key-here"

# App Password (required for public deployments)
APP_PASSWORD = "your-secure-password-here"
